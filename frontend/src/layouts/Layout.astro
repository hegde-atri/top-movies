---
import Footer from "../components/Footer.astro";
import Head from "../components/Header/Head.astro";
import Navbar from "../components/Header/Navbar.astro";

export interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
    <Head title={title} />
    <body>
		<Navbar />
        <main>
            <slot />
        </main>
		<Footer />
    </body>
</html>
